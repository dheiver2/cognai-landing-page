{
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "CI": "false",
    "GENERATE_SOURCEMAP": "false"
  }
}
